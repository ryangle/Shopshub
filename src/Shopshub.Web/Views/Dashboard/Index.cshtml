@section Styles{
    <link rel="stylesheet" href="echarts/css/index.css" />
}
@section Scripts{
    <script src="echarts/js/flexible.js"></script>
    <script src="echarts/js/jquery-3.4.1/jquery-3.4.1.min.js"></script>
    <script src="echarts/4.8.0/echarts.min.js"></script>
    <script type="text/javascript">
        // 基于准备好的dom，初始化echarts实例
        var myChart = echarts.init($('#mainchart1')[0]);

        // 指定图表的配置项和数据
        var option = {
            title: {
                text: 'ECharts 入门示例'
            },
            tooltip: {},
            legend: {
                data: ['销量']
            },
            xAxis: {
                data: ["衬衫", "羊毛衫", "雪纺衫", "裤子", "高跟鞋", "袜子"]
            },
            yAxis: {},
            series: [{
                name: '销量',
                type: 'line',
                data: [5, 20, 36, 10, 10, 20]
            }]
        };

        // 使用刚指定的配置项和数据显示图表。
        myChart.setOption(option);
    </script>
}
